<script>
function onlogin(){
	var userName = $('userName').val();
	var passWord = $('passWord').val();
	
	$.ajax({
		type: "POST",
		url:"loginTeacher.do",
		data:{
			"userName":userName, "passWord":passWord
		},
		sucess:function (data){
			if(data=="sucess"){
				window.location = "adminListinformation.do";
			}else{
                alert('login agarin');
            }
			
		},
		error:function(xhr, status, error) {
			console.log('worng')
			console.log(error);
		}
	});
	
}

</script>
